<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../head.ejs') %>
</head>
<body>
    <%- include('../header.ejs') %>
    <div class="wrapper">
        <div class="channel-head">
            <div class="banner">
                <div class="cover unselectable"><img id="cover"></div>
                <div class="info">
                    <div class="thumbnail unselectable"><img id="thumbnail"></div>
                    <div class="text">
                        <span id="title"></span>
                        <span id="name"></span>
                        <span id="claims"></span>
                    </div>
                </div>
                <button id="follow"></button>
            </div>
            <div class="pages">
                <button id="claims-button">Claims</button>
                <button id="about-button">About</button>
            </div>
        </div>
        <div id="notice"></div>
        <div id="channel-about">
            
        </div>
        <div id="channel-claims">
            <%- include('../feed.ejs', {type: "channel", info}) %> 
        </div>
    </div>
    <%- include('./script.ejs') %>
</body>
</html>